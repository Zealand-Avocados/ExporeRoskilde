@page "{id}"
@model ExploreRoskilde.Pages.Places.RemovePlace

@{
   ViewData["Title"] = Model.Place.Title;
}

<h1>Remove page</h1>

<h1>Delete Confirmation></h1>
<div class="alert alert-danger">
    <h5>Are you sure you want to delete Item - @Model.Place.Title</h5>
    <form method="post">
        <button type="submit" class="btn btn-danger">Yes</button>
        <a class="btn btn-primary" asp-page="/Index">No</a>
    </form>
</div>
